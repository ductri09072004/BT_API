      {
        "id": 1,
        "title": "Service Status Overview",
        "type": "table",
        "targets": [
          {
            "expr": "kube_pod_status_phase{namespace=\"bt-api\"}",
            "legendFormat": "{{pod}} - {{phase}}",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            },
            "refId": "A",
            "queryType": "timeSeriesQuery"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "table"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        }
      },
      {
        "id": 3,
        "title": "Order Service - CPU & Memory",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"bt-api-services\", instance=~\"host.docker.internal:8001\"}[5m]) * 100",
            "instant": false,
            "legendFormat": "Order CPU Usage %",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            },
            "refId": "A",
            "queryType": "timeSeriesQuery"
          },
          {
            "expr": "process_resident_memory_bytes{job=\"bt-api-services\", instance=~\"host.docker.internal:8001\"} / 1024 / 1024",
            "instant": false,
            "legendFormat": "Order Memory Used (MB)",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            },
            "refId": "B",
            "queryType": "timeSeriesQuery"
          },
          {
            "expr": "process_virtual_memory_bytes{job=\"bt-api-services\", instance=~\"host.docker.internal:8001\"} / 1024 / 1024",
            "instant": false,
            "legendFormat": "Order Memory Virtual (MB)",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            },
            "refId": "C",
            "queryType": "timeSeriesQuery"
          },
          {
            "expr": "python_gc_objects_collected_total{job=\"bt-api-services\", instance=~\"host.docker.internal:8001\"}",
            "instant": false,
            "legendFormat": "Order GC Objects",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            },
            "refId": "D",
            "queryType": "timeSeriesQuery"
          },
          {
            "expr": "up{job=\"bt-api-services\", instance=~\"host.docker.internal:8001\"}",
